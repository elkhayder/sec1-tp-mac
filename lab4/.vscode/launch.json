    "version": "0.2.0",
    "configurations": [
    {
        "cwd": "${workspaceFolder}",
        "executable": "./build/lab4",
        "name": "Debug STM32 lab4",
        "request": "launch", //"launch" => reload firmware, "attach" => just start debug session
        "type": "cortex-debug",
        "showDevDebugOutput": "none",
        "servertype": "stutil",
        "runToEntryPoint": "main",
        "device": "STM32F303K8",
        "v1": false,
        "svdFile": "${workspaceFolder}/../sys/CMSIS/SVD/STM32F303x.svd",
    }
    ]
}
